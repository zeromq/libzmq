AC_MSG_RESULT([     Java: $jzmq])
AC_MSG_RESULT([   Transports:])
AC_MSG_RESULT([     tcp: yes])
AC_MSG_RESULT([     udp: $pgm_ext])
AC_MSG_RESULT([     pgm: $pgm_ext])
AC_MSG_RESULT([     inproc: yes])
AC_MSG_RESULT([   Devices:])
AC_MSG_RESULT([     Forwarder: $forwarder])
